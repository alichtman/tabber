{
  "manifest_version": 2,
  "name": "tabber",
  "version": "0.1",
  "background": {
    "scripts": ["extension/background.js"]
  },
  "content_scripts": [{
    "matches": [
      "http://messenger.com/*",
      "https://messenger.com/*",
      "http://www.messenger.com/*",
      "https://www.messenger.com/*"
    ],
    "js": ["extension/assets/jquery-3.2.1.min.js", "extension/content/save.js", "extension/content/manager.js"]
  }],
  "browser_action": {
    "default_icon": "extension/assets/icon.png",
    "default_title": "tabber"
  },
  "permissions": [
    "contextMenus",
    "http://localhost:5000/*"
  ],
  "web_accessible_resources": []
}
