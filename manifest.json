{
  "key": "",
  "name": "tabber",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "http://localhost:5000/tabber/api/*",
    "identity"
  ],
  "oauth2": {
    "client_id": "557897042954-o3obikdtoprmoungto84a0jkgum3cahv.apps.googleusercontent.com",
    "scopes": ["profile"]
  },
  "browser_action": {
    "default_icon": "extension/assets/icon.png",
    "default_title": "tabber"
  },
  "web_accessible_resources": [],
  "background": {
    "scripts": ["extension/background.js"]
  },
  "content_scripts": [{
    "matches": [
      "http://messenger.com/*",
      "https://messenger.com/*",
      "http://www.messenger.com/*",
      "https://www.messenger.com/*"
    ],
    "js": ["extension/assets/jquery-3.2.1.min.js", "extension/content/save.js", "extension/content/manager.js", "extension/content/register.js", "extension/content/onboarding.js"]
  }]
}
