{
  "manifest_version": 2,
  "name": "messagebook",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": [
      "http://messenger.com/*",
      "https://messenger.com/*",
      "http://www.messenger.com/*",
      "https://www.messenger.com/*"
    ],
    "js": ["jquery-3.2.1.min.js", "content.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "messagebook"
    //"default_popup": "popup.html"
  }
}